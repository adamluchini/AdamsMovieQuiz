@model MovieObjectTest.Models.ArgoClass
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.0/jquery.min.js"></script>

<h3>The top billed actors were @ViewBag.movieResult["Actors"]</h3>
<button class="clue3">Clue #3</button>
<script type="text/javascript">
    $('.clue3').click(function () {
        $.ajax({
            type: 'GET',
            dataType: 'html',
            url: '@Url.Action("ArgoAnswer3")',
            success: function (result) {
                $('#argo-set-3').html(result);
            }
        });
    });
</script>



