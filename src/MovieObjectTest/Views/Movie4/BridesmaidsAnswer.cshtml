@model MovieObjectTest.Models.BridesmaidsClass
<h3>Released in @ViewBag.movieResult["Year"], this @ViewBag.MovieResult["Genre"] movie was directed by @ViewBag.movieResult["Director"]</h3>
<button class="clue2">Clue #2</button>
<script type="text/javascript">
    $('.clue2').click(function () {
        $.ajax({
            type: 'GET',
            dataType: 'html',
            url: '@Url.Action("BridesmaidsAnswer2")',
            success: function (result) {
                $('#bridesmaids-set-2').html(result);
            }
        });
    });
</script>




